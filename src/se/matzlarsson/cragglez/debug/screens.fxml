<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="se.matzlarsson.cragglez.debug.ScreensController">

    <Label fx:id="debugLabel" alignment="TOP_CENTER" contentDisplay="CENTER" text="Screen Data Here" textAlignment="CENTER" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="50">
        <font>
            <Font size="24.0" />
        </font>
    </Label>
    <Button alignment="CENTER" contentDisplay="CENTER" layoutX="330.0" layoutY="320.0" mnemonicParsing="false" onAction="#reloadScreens" prefHeight="50.0" prefWidth="90.0" text="Reload">
        <font>
            <Font size="18.0" />
        </font>
    </Button>
    <Button alignment="CENTER" contentDisplay="CENTER" layoutX="180.0" layoutY="320.0" mnemonicParsing="false" onAction="#close" prefHeight="50.0" prefWidth="90.0" text="Close">
        <font>
            <Font size="18.0" />
        </font>
    </Button>

</AnchorPane>
